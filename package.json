{
  "name": "python-inline-sql-syntax",
  "displayName": "Python Inline Sql Syntax",
  "description": "Add SQL syntax highlighting for Python strings.",
  "publisher": "chdsbd",
  "homepage": "https://github.com/chdsbd/vscode-python-inline-sql-syntax/blob/main/README.md",
  "icon": "images/logo256.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/chdsbd/vscode-python-inline-sql-syntax.git"
  },
  "bugs": {
    "url": "https://github.com/chdsbd/vscode-python-inline-sql-syntax/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "sql",
    "highlight",
    "inline"
  ],
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/highlight-sql.json",
        "scopeName": "highlight-sql.injection",
        "injectTo": [
          "source.python"
        ],
        "embeddedLanguages": {
          "meta.embedded.sql": "sql"
        }
      }
    ]
  }
}
